def main():
    numbers = []
    
    while True:
        try:
            num = int(input("Enter a number (or other character to stop): "))
            numbers.append(num)
        except ValueError:
            print("\nStopping...")
            break
    
    if numbers:
        print(f"\nFinal lowest number: {min(numbers)}")

if __name__ == "__main__":
    main()
